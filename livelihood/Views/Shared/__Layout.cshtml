    <script>
        $(document).ready(function () {
            
            $('#btn-filter-listing').prop('disabled', true);
            $('#reset-filter').prop('disabled', true);
            $("#btn-filter-listing").css('cursor', 'no-drop');
            $("#reset-filter").css('cursor', 'no-drop');
            $('#listing-filter-modal input, #listing-filter-modal select').on('select change', function () {
                console.log('----------', $(this).val());
                if ($(this).val() != '' && $(this).val() != null) {
                    $('#btn-filter-listing').prop('disabled', false);
                    $('#reset-filter').prop('disabled', false);
                    $("#btn-filter-listing").css('cursor', 'pointer');
                    $("#reset-filter").css('cursor', 'pointer');

                } else {
                    $('#btn-filter-listing').prop('disabled', true);
                    $('#reset-filter').prop('disabled', true);
                    $("#btn-filter-listing").css('cursor', 'no-drop');
                    $("#reset-filter").css('cursor', 'no-drop');

                }
            });
        });
    </script>
