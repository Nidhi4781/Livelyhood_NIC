<!-- ======= Counts Section ======= -->
<section id="counts" class="counts">
    <div class="container-fluid" data-aos="fade-up">
        <div class="row">
            <div class="col-lg-2 col-md-6 mt-5 mt-lg-0">
                <div class="count-box sec-card">
                    <i class="icofont-university "></i>
                    <span data-toggle="counter-up"><h2 style="padding:10px">@ViewData["TotalLC"]</h2></span>
                    <a asp-controller="APO" asp-action="LivelihoodColleges">Livelihood Colleges</a>
                </div>
            </div>
            <div class="col-lg-2 col-md-6  mt-5 mt-lg-0">
                <div class="count-box sec-card">
                    <i class="icofont-graduate "></i>
                    <span data-toggle="counter-up"><h2 style="padding:10px"><Label ID="lbl_total_lcer" style="font-size:2rem!important;"> @ViewData["TotalCertified"] </Label></h2></span>
                    <p style="text-align:center!important; font-weight:bold;">Certified Beneficiaries</p>
                </div>
            </div>
            <div class="col-lg-2 col-md-6 mt-5 mt-lg-0">
                <div class="count-box sec-card">
                    <i class="icofont-group-students "></i>
                    <span data-toggle="counter-up"><h2 style="padding:10px"><Label ID="lbl_total_lreg" style="font-size:2rem!important;"> @ViewData["TotalReg"] </Label></h2></span>
                    <p style="text-align:center!important; font-weight:bold;">Registered Beneficiaries</p>
                </div>
            </div>
            <div class="col-lg-2 col-md-6 mt-5 mt-lg-0">
                <div class="count-box sec-card">
                    <i class="icofont-education "></i>
                    <span data-toggle="counter-up"><h2 style="padding:10px"><Label ID="lbl_total_lcou" style="font-size:2rem!important;"> @ViewData["Totalcourse"] </Label></h2></span>
                    <p style="text-align:center!important; font-weight:bold;">Registered Courses</p>
                </div>
            </div>

            <div class="col-lg-2 col-md-6 mt-5 mt-lg-0">
                <div class="count-box sec-card">
                    <i class="icofont-engineer "></i>
                    <span data-toggle="counter-up"><h2 style="padding:10px"><Label ID="lbl_total_lcou" style="font-size:2rem!important;"> @ViewData["Totalplaced"] </Label></h2></span>
                    <p style="text-align:center!important; font-weight:bold;">Placed Beneficiaries</p>
                </div>
            </div>

            <div class="col-lg-2 col-md-6 mt-5 mt-lg-0">
                <div class="count-box sec-card">
                    <i class="icofont-architecture-alt "></i>
                    <span data-toggle="counter-up"><h2 style="padding:10px"><Label ID="lbl_total_lun" style="font-size:2rem!important;"> @ViewData["TotalUnder"] </Label></h2></span>
                    <p style="text-align:center!important; font-weight:bold;">Undertraining Beneficiaries</p>
                </div>
            </div>
        </div>

    </div>
</section>
<!-- End Counts Section -->
